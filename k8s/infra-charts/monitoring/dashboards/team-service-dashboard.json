{
  "title": "TeamService",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-30m", "to": "now" },
  "timezone": "",
  "tags": ["team-service", "prometheus"],
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests / sec (RPS)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(teamservice_http_requests_total{job=\"team-service\"}[5m]))",
          "legendFormat": "rps"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx error rate (%)",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (sum(rate(teamservice_http_requests_total{job=\"team-service\",code=~\"5..\"}[5m])) / sum(rate(teamservice_http_requests_total{job=\"team-service\"}[5m])))",
          "legendFormat": "5xx %"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (seconds)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(teamservice_http_request_duration_seconds_bucket{job=\"team-service\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p99 (seconds)",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum(rate(teamservice_http_request_duration_seconds_bucket{job=\"team-service\"}[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "In-flight requests",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "teamservice_http_in_flight_requests{job=\"team-service\"}",
          "legendFormat": "in_flight"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Top routes by RPS",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(8, sum(rate(teamservice_http_requests_total{job=\"team-service\"}[5m])) by (route))",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 by route (top 8)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 24, "w": 24, "h": 10 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(8, histogram_quantile(0.95, sum(rate(teamservice_http_request_duration_seconds_bucket{job=\"team-service\"}[5m])) by (le, route)))",
          "legendFormat": "{{route}}"
        }
      ]
    }
  ]
}
